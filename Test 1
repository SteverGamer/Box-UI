local UI = {}
function UI.createWindow(name)
	--ScreenGui1--
	local ScreenGui1 = Instance.new("ScreenGui",game.CoreGui)
	ScreenGui1["Name"] = "Window"
	ScreenGui1["ZIndexBehavior"] = Enum.ZIndexBehavior.Sibling
	----ScreenGui1----
	--Frame2--
	local Frame2 = Instance.new("Frame",ScreenGui1)
	Frame2["Name"] = "DragBar"
	Frame2["Position"] = UDim2.new(0.281308413, 0, 0.368663609, 0)
	Frame2["Size"] = UDim2.new(0, 444, 0, 24)
	Frame2["ZIndex"] = 2
	Frame2["BorderSizePixel"] = 0
	Frame2["BackgroundColor3"] = Color3.new(0.352941, 0.352941, 0.352941)
	----Frame2----
	--Frame3--
	local Frame3 = Instance.new("Frame",Frame2)
	Frame3["Name"] = "Blocker"
	Frame3["Position"] = UDim2.new(-0.000223108247, 0, 0.551450074, 0)
	Frame3["Size"] = UDim2.new(0, 444, 0, 10)
	Frame3["ZIndex"] = 2
	Frame3["BorderSizePixel"] = 0
	Frame3["BackgroundColor3"] = Color3.new(0.352941, 0.352941, 0.352941)
	----Frame3----
	--UICorner4--
	local UICorner4 = Instance.new("UICorner",Frame2)
	UICorner4["CornerRadius"] = UDim.new(0, 9)
	----UICorner4----
	--Frame5--
	local Frame5 = Instance.new("Frame",Frame2)
	Frame5["Size"] = UDim2.new(0, 444, 0, 249)
	Frame5["Position"] = UDim2.new(-0.000223039504, 0, 0.66036731, 0)
	Frame5["Name"] = "Background"
	Frame5["BackgroundColor3"] = Color3.new(0.243137, 0.243137, 0.243137)
	----Frame5----
	--UICorner6--
	local UICorner6 = Instance.new("UICorner",Frame5)
	UICorner6["CornerRadius"] = UDim.new(0, 9)
	----UICorner6----
	--TextLabel7--
	local TextLabel7 = Instance.new("TextLabel",Frame2)
	TextLabel7["TextColor3"] = Color3.new(0, 0, 0)
	TextLabel7["Text"] = name
	TextLabel7["TextXAlignment"] = Enum.TextXAlignment.Left
	TextLabel7["Size"] = UDim2.new(0, 409, 0, 19)
	TextLabel7["Font"] = Enum.Font.JosefinSans
	TextLabel7["Name"] = "Title"
	TextLabel7["Position"] = UDim2.new(0.00675675692, 0, 0.166666672, 0)
	TextLabel7["BackgroundTransparency"] = 1
	TextLabel7["ZIndex"] = 3
	TextLabel7["TextSize"] = 14
	TextLabel7["BackgroundColor3"] = Color3.new(1, 1, 1)
	----TextLabel7----
	--TextButton8--
	local TextButton8 = Instance.new("TextButton",Frame2)
	TextButton8["TextWrapped"] = true
	TextButton8["TextColor3"] = Color3.new(1, 0, 0)
	TextButton8["Text"] = "X"
	TextButton8["TextSize"] = 14
	TextButton8["Size"] = UDim2.new(0, 17, 0, 17)
	TextButton8["Font"] = Enum.Font.Unknown
	TextButton8["Name"] = "Close"
	TextButton8["Position"] = UDim2.new(0.961711705, 0, 0.166666672, 0)
	TextButton8["BackgroundTransparency"] = 1
	TextButton8["ZIndex"] = 3
	TextButton8["TextScaled"] = true
	TextButton8["BackgroundColor3"] = Color3.new(1, 1, 1)
	----TextButton8----
	--Script9--
	local Script9 = Instance.new("Script",TextButton8)
	----Script9----
	spawn(function() --Source for Script9
		local script = Script9
		script.Parent.Activated:Connect(function()
			script.Parent.Parent.Parent:Destroy()
		end)
	end)
	function UI.createButton(name)
		
	end
end
return UI
